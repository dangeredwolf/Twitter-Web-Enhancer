{
  "name": "Twitter Web Enhancer",
  "short_name": "Twitter Web Enhancer",

  "description": "Making twitter.com suck lessâ„¢ Also yeah ill make a fresh new icon for this thing soon",
  "version": "0.1.2",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*.twitter.com/"
  ],

  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*","https://twitter.com/*"],
      "js": ["extension/TWELoad.js"],
      "run_at": "document_end"
    }
  ],

  "background": {
    "scripts": ["extension/TWEBackground.js"],
    "persistent": true
  },

  "icons": {
      "16": "AppIcon16.png",
      "24": "AppIcon24.png",
      "32": "AppIcon32.png",
      "48": "AppIcon48.png",
      "64": "AppIcon64.png",
      "128": "AppIcon128.png",
      "256": "AppIcon256.png"
   },

  "web_accessible_resources": [
    "resources",
    "resources/*"
  ],

  "manifest_version": 2
}
